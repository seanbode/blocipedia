<h1>Wiki Index</h1>
<br />
<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
          <small>
            <%= wiki.body %>
          </small>
        </div>
      </div>
    <% end %>
  </div>
  <br />
  <div class="col-md-4">
    <%= link_to "New Entry", new_wiki_path, class: 'btn btn-success' %>
  </div>
</div>
